        <PackageReference Include="Microsoft.NET.ILLink.Tasks" Version="8.0.4" />
        <PackageReference Include="System.Net.Http.Json" Version="8.0.0" />
        <PackageReference Include="Toolbelt.Blazor.HeadElement" Version="7.3.1" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="wwwroot\assets" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="Localizations\Shared\NavMenu.resx">
